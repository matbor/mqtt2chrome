{
   "background": {
      "scripts": [ "mqttws31.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon_noconnection.png",
      "default_title": "MQTT2Chrome"
   },
   "description": "Displays MQTT messages as Chrome notifications, using MQTT over websockets",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "mqtt2chrome",
   "options_page": "options.html",
   "permissions": [ "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.2",
   "web_accessible_resources": [ "icon.png" ]
}
